    "scripts": {
        "dev": "next dev",
        "dev:server": "node server.js",
        "build": "cross-env NODE_OPTIONS=--max_old_space_size=3072 && next build",
        "build:start": "npm run build && next start",
        "build:export": "cross-env NEXT_EXPORT=true npm run build && cross-env NEXT_EXPORT=true next export -o dist",
        "start": "next start",
        "start:worker": "cross-env SERVICE_WORKER=true npm run start",
        "lint": "next lint --fix",
        "format": "prettier --write \"**/*.+(ts|tsx|js|jsx|json|yml|yaml|css|scss)\"",
        "prepare": "husky install",
        "graphql-clear-types": "rimraf ./src/**/gqlTypes && rimraf ./src/next/graphql/schema.tsx",
        "graphql-generate-types": "dotenv -c -- graphql-codegen --config codegen.yml",
        "codegen": "apollo codegen:generate --target=typescript --tagName=gql gqlTypes && npm run format",
        "generate": "plop",
        "extract-json-messages": "rimraf dist/locale && cross-env NODE_ENV=extract babel src 'src/**/*.{ts,tsx}' -o /Projects/mzawadie/mzawadie-storefront/dev/null",
        "transpile-messages": "node scripts/transpile-tx.js",
        "extract-messages": "npm run extract-json-messages && npm run transpile-messages",
        "storybook": "start-storybook -p 9009 -s ./public",
        "build-storybook": "build-storybook -s public -o ./dist/storybook",
        "cy:run": "cypress run",
        "cy:run:record": "npm run cy:run -- --record",
        "cy:open": "cypress open",
        "test": "cross-env LANGUAGE_LOCALE=en-US jest --runInBand --config .jest/config.json --passWithNoTests",
        "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
        "test:e2e:run:record": "start-server-and-test start http://localhost:3000 cy:run:record",
        "test:e2e:dev": "start-server-and-test start http://localhost:3000 cy:open"
    },
    "lint-staged": {
        "*.+(ts|tsx|js|jsx)": [
            "eslint 'src/**/*.{ts,tsx,js,jsx}' --quiet --fix",
            "git add -A ."
        ],
        "*.+(json|css|md)": [
            "prettier --write",
            "git add -A ."
        ],
        "*.+(ts|tsx|js|jsx|json|yml|yaml|css|md|html)": "prettier --write",
        "*.js": "eslint --cache --fix",
        "*.+(ts|tsx|js|jsx|json|yml|yaml|css)": "prettier --write"
    }
