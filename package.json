{
    "name": "mzawadie-storefront",
    "version": "1.0.0",
    "private": true,
    "author": "Emmanuel Maro <emmanuelmaro733@gmail.com>",
    "browser": {
        "fs": false,
        "path": false,
        "process": false,
        "os": false
    },
    "scripts": {
        "dev": "next dev -p 9000",
        "build": "next build",
        "start": "next start -p 9000",
        "lint": "next lint --fix --no-cache",
        "format": "prettier --write \"**/*.+(ts|tsx|js|jsx|json|yml|yaml|css|scss)\"",
        "prepare": "npm link @mzawadie/sdk && husky install",
        "codegen-graphql": "graphql-codegen --config codegen.yml",
        "codegen": "apollo codegen:generate --target=typescript --tagName=gql gqlTypes && npm run format",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "generate": "plop",
        "extract-json-messages": "rimraf dist/locale && cross-env NODE_ENV=extract babel src 'src/**/*.{ts,tsx}' -o /Projects/mzawadie/mzawadie-storefront/dev/null",
        "transpile-messages": "node scripts/transpile-tx.js",
        "extract-messages": "npm run extract-json-messages && npm run transpile-messages"
    },
    "lint-staged": {
        "*.+(ts|tsx|js|jsx)": [
            "eslint 'src/**/*.{ts,tsx,js,jsx}' --quiet --fix",
            "git add"
        ],
        "*.+(json|css|md)": [
            "prettier --write",
            "git add"
        ],
        "*.+(ts|tsx|js|jsx|json|yml|yaml|css|md|html)": "prettier --write",
        "*.js": "eslint --cache --fix",
        "*.+(ts|tsx|js|jsx|json|yml|yaml|css)": "prettier --write"
    },
